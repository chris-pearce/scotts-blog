[ignore]
<PROJECT_ROOT>/.vscode/.*
<PROJECT_ROOT>/public/.*
<PROJECT_ROOT>/config/.*

[include]

[libs]

[lints]

[options]
module.name_mapper='^\(.*\)$' -> '<PROJECT_ROOT>/\1'
module.use_strict=true
esproposal.decorators=ignore
emoji=true
suppress_comment=\\(.\\|\n\\)*\\$FlowExpectError
suppress_comment=\\(.\\|\n\\)*\\$FlowIgnore
suppress_comment=\\(.\\|\n\\)*\\$FlowFixMe

[strict]
